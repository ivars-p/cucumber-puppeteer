{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "lib": ["ES2020", "DOM"],
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "outDir": "dist",
    "types": ["node"],
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "noImplicitAny": false

  },
  "include": ["../cucumber-puppeteer/features/**/*.ts"],
  "exclude": ["node_modules", "dist"]
}